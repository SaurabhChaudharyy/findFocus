{
  "manifest_version": 3,
  "name": "Website Blocker",
  "version": "1.0",
  "description": "Block websites specified by the user.",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "popup.js"
  },
  "options_ui": {
    "page": "popup.html",
    "open_in_tab": false
  }
}
